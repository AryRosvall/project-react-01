@import "../vars.styl"

.Header
  background-color blue
  display flex
  align-items center
  justify-content center
  grid-column 1 / 2
  place-self center
  padding 0

.Header__profile-photo
  width 150px
  height 150px
  border-radius 10%
  -o-object-fit cover
  object-fit cover
  border 5px solid body-color